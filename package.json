    "commander": "^9.2.0",
    "discord-api-types": "^0.32.0",
    "discord.js": "^13.6.0",
    "lambda-logger-node": "4.0.0-7",
    "tweetnacl": "^1.0.3",
    "twitter-api-v2": "^1.12.0",
