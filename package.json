    "discord:cli": "yarn discord:cli:build && node .layers/discord-cli/index.cjs",
    "discord:cli:build": "esbuild src/cli.ts --platform=node --target=node16.14 --bundle --outfile=.layers/discord-cli/index.cjs",
    "discord:build": "esbuild src/lambda/discord.ts --platform=node --target=node16.14 --minify --bundle --outfile=.layers/discord/index.js",
    "ingest:build": "esbuild src/lambda/ingest.ts --platform=node --target=node16.14 --minify --bundle --outfile=.layers/ingest/index.js",
    "deferred:build": "esbuild src/lambda/deferred.ts --platform=node --target=node16.14 --minify --bundle --outfile=.layers/deferred/index.js",
    "test": "jest test",
    "open:gen": "openapi --input swagger.json --client axios --output ./src/swagger-gen"
  },
  "devDependencies": {
    "@shelf/jest-dynamodb": "^2.2.4",
    "@types/aws-lambda": "^8.10.95",
